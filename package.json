{
  "name": "code-on-track-root",
  "version": "1.0.0",
  "description": "Railway QR Code Tracking System - Root Package",
  "scripts": {
    "install-all": "npm run install-backend && npm run install-frontend && npm run install-mobile && npm run install-qr",
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend && npm install",
    "install-mobile": "cd mobile && npm install",
    "install-qr": "cd qr-generation && npm install",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd frontend && npm run dev",
    "start-mobile": "cd mobile && npx expo start",
    "dev-backend": "cd backend && npm run dev",
    "test-backend": "cd backend && npm test"
  },
  "keywords": ["railway", "qr-code", "tracking", "llm", "sih-2025"],
  "author": "SIH 2025 Team",
  "license": "MIT"
}
